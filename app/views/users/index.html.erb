<%- content_for :header do -%>
	<div id="page_info">
		<h2>ユーザ一覧</h2>
	</div>
<%- end -%>

<table id="users">
	<tr>
		<th class="name">ユーザ名</th>
	</tr>
	<%- @users.each do |user| -%>
	<tr class="<%= cycle('odd', 'even') %>">
		<td class="name"><%= link_to_user h(user.name), user %></td>
	</tr>
	<%- end -%>
</table>
